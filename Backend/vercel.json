{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "API_KEY": "@api-key"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
